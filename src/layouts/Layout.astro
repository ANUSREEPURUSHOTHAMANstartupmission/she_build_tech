    <link rel="stylesheet" href="assets/css/plugins/swiper-bundle.min.css" />


    <!-- Style CSS -->
    <link rel="stylesheet" href="assets/css/style.css" />
	<link href="https://cdn.jsdelivr.net/npm/flowbite@3.1.2/dist/flowbite.min.css" rel="stylesheet" />

</head>

	<body>
		<Navbar client:load></Navbar>
		<slot />

    <!-- Vendors JS -->
    <script src="assets/js/vendor/modernizr-3.11.7.min.js" is:inline></script>
    <script src="assets/js/vendor/jquery-3.6.0.min.js" is:inline></script>
    <script src="assets/js/vendor/jquery-migrate-3.3.2.min.js" is:inline></script>
    <!-- Plugins JS -->
    <script src="assets/js/plugins/aos.js" is:inline>
    </script>
    <script src="assets/js/plugins/swiper-bundle.min.js" is:inline>
    </script>
    <script src="assets/js/plugins/jquery.waypoints.js" is:inline>
    </script>
